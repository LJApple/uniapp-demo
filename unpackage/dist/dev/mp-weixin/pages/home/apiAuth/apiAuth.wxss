.api-auth .aa-list {
  padding: 42rpx 31rpx 0 31rpx;
}
.api-auth .aa-list .aal-ul {
  background-color: #fff;
  border-radius: 15.38rpx;
  padding: 0 31rpx;
}
.api-auth .aa-list .aal-ul .aalu-li {
  height: 96rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-top: 1rpx solid #dadbde;
}
.api-auth .aa-list .aal-ul .aalu-li:first-child {
  border: 0;
}
.api-auth .aa-list .aal-ul .aalu-li .aalul-left {
  font-size: 30.77rpx;
  font-weight: 400;
  font-family: "苹方-简";
}
.api-auth .aa-list .aal-ul .aalu-li .aalul-right {
  display: flex;
  align-items: center;
}
.api-auth .aa-list .aal-ul .aalu-li .aalul-right .aalulr-text {
  padding-right: 10rpx;
  color: #999999;
  font-size: 26.92rpx;
}
.api-auth .aa-list .aal-ul .aalu-li .aalul-right .isActive {
  color: #FFB119;
}